  "id" : "000828925",
  "numeroProcesso" : "67255",
  "numeroRegistro" : "202102793108",
  "siglaClasse" : "AgInt no RMS",
  "descricaoClasse" : "AGRAVO INTERNO NO RECURSO EM MANDADO DE SEGURANÇA",
